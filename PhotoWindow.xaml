<Window x:Class="Autocare_WPF.PhotoWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Autocare_WPF"
        mc:Ignorable="d"
        Title="PhotoWindow" Height="450" Width="800">
    <Grid>
        <Image x:Name="photoImage" HorizontalAlignment="Center" VerticalAlignment="Center" Stretch="Uniform" />
    </Grid>
</Window>
